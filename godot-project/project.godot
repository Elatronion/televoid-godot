config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Hotspot",
"language": "GDScript",
"path": "res://custom_nodes/Hotspot.gd"
}, {
"base": "Resource",
"class": "TootlDialogue",
"language": "GDScript",
"path": "res://custom_resources/TootlDialogue.gd"
}, {
"base": "Area2D",
"class": "Trigger",
"language": "GDScript",
"path": "res://custom_nodes/Trigger.gd"
} ]
_global_script_class_icons={
"Hotspot": "",
"TootlDialogue": "res://custom_resources/message-square.svg",
"Trigger": ""
}

[application]

config/name="Televoid: Out of The Loop - Godot Edition"
run/main_scene="res://scenes/prefabs/SceneManager/SceneManager.tscn"
config/icon="res://icon.png"

[autoload]

GameManager="*res://global/game_manager.gd"
GlobalInput="*res://global/global_input.gd"

[display]

window/size/width=1280
window/size/height=720

[editor_plugins]

enabled=PoolStringArray( "res://addons/elatronion.imv_importer/plugin.cfg", "res://addons/elatronion.tootl_csv_dialogue_importer/plugin.cfg", "res://addons/elatronion.wren_importer/plugin.cfg", "res://addons/vnen.tiled_importer/plugin.cfg" )

